{
  "name": "electrowat.ch",
  "version": "1.0.0",
  "description": "A electron io version of cryptowat.ch ember",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrootkit/electrowat.ch.git"
  },
  "keywords": [
    "crypto",
    "node",
    "rxjs",
    "nest"
  ],
  "author": "m.rootkit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrootkit/electrowat.ch/issues"
  },
  "homepage": "https://github.com/mrootkit/electrowat.ch#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^8.0.34",
    "chai": "^4.1.2",
    "electron": "^1.7.9",
    "electron-builder": "^19.43.0",
    "electron-mocha": "^4.0.3",
    "electron-rebuild": "^1.6.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.3.2",
    "gulp-plumber": "^1.1.0",
    "gulp-typescript": "^3.2.3",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "istanbul": "^0.4.5",
    "minimist": "^1.2.0",
    "mocha": "^4.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.5.0",
    "spectron": "^3.7.2"
  },
  "dependencies": {
    "cryptowatch-embed": "^0.3.2",
    "electron": "^1.7.9",
    "fs-jetpack": "^1.2.0",
    "lodash": "^4.17.4",
    "run-sequence": "^2.2.0",
    "rxjs": "^5.5.2",
    "typescript": "^2.6.1"
  }
}
